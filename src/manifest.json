{
  "manifest_version": 2,
  "name": "Proxy Switcher",
  "version": "1.0",
  "description": "Easily switch between proxy settings",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "incognito":"spanning",
  "author": "Francesco Varotto",
  "homepage_url": "https://github.com/francevarotz98"
}
